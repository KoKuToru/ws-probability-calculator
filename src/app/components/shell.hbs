{{#if this.state.loaded}}
  <div class="code-section">
    <Section>
      <:header>
        <span>Code</span>
      </:header>
      <:default>
        <Code />
      </:default>
    </Section>
  </div>
  <div class="overview-section">
    <Section>
      <:header>
        <span>Overview</span>
        <span>weighted average damage</span>
      </:header>
      <:default>
        <Overview />
      </:default>
    </Section>
  </div>
  {{#if this.state.hasSelected}}
    <div class="probability-section">
      <Section>
        <:header>
          <span>Probabilites</span>
          <span>%</span>
        </:header>
        <:default>
          <Details />
        </:default>
      </Section>
    </div>
  {{/if}}
{{else}}
  Loading..
{{/if}}