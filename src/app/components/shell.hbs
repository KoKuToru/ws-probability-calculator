{{#if this.state.loaded}}
  <div class="header-section">
    <summary>
      <span>Wei√ü Schwarz</span>
      <span>Probability Calculator BETA</span>
      <span class="flex-grow"></span>
      {{#if this.canShare}}
        <span><button {{on "click" this.share}}>share</button></span>
      {{/if}}
      <span><button {{on "click" this.help}}>help</button></span>
      <span><button {{on "click" this.reset}}>reset</button></span>
    </summary>
  </div>
  <div class="code-section">
    <Code/>
  </div>
  <div class="code-section">
    <Section @name="debug_open">
      <:header>
        <span>Compiled Code</span>
      </:header>
      <:default>
        <div class="code code-compiled">
          <code>{{this.state.debug_code}}</code>
        </div>
      </:default>
    </Section>
  </div>
  <div class="overview-section">
    <Overview />
  </div>
  <div class="probability-section">
    <Probability />
  </div>
{{else}}
  Loading..
{{/if}}